{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "SmartRAG": "Information",
      "SmartRAG.Services": "Information",
      "SmartRAG.Services.AudioConversionService": "Information",
      "SmartRAG.Services.WhisperAudioParserService": "Information",
      "Microsoft": "Information",
      "System": "Information"
    }
  },
  "SmartRAG": {
    "MaxChunkSize": 1000,
    "MinChunkSize": 100,
    "ChunkOverlap": 200,
    "MaxRetryAttempts": 3,
    "RetryDelayMs": 1000,
    "RetryPolicy": "ExponentialBackoff",
    "EnableFallbackProviders": false,
    "FallbackProviders": [],
    "MaxSearchResults": 10,
    "EnableAutoSchemaAnalysis": true,
    "EnablePeriodicSchemaRefresh": false,
    "DefaultSchemaRefreshIntervalMinutes": 60,
    "ConversationStorageProvider": "Redis",
    "WhisperConfig": {
      "ModelPath": "models/ggml-large-v3.bin",
      "DefaultLanguage": "auto",
      "MinConfidenceThreshold": 0.3,
      "PromptHint": "",
      "MaxThreads": 0
    },
    "Features": {
      "EnableDatabaseSearch": false,
      "EnableDocumentSearch": true,
      "EnableAudioSearch": true,
      "EnableImageSearch": true,
      "EnableMcpSearch": true,
      "EnableFileWatcher": true
    },
    "McpServers": [
      {
        "ServerId": "Server1",
        "Endpoint": "https://mcp-server1.example.com/endpoint",
        "AutoConnect": false,
        "TimeoutSeconds": 30,
        "Headers": {
          "Authorization": "Bearer your-mcp-token"
        }
      }
    ],
    "WatchedFolders": [
      {
        "FolderPath": "Documents",
        "AllowedExtensions": [ ".pdf", ".docx", ".txt" ],
        "IncludeSubdirectories": true,
        "AutoUpload": false,
        "UserId": "system"
      }
    ]
  },
  "DatabaseConnections": [
    {
      "Name": "Database1",
